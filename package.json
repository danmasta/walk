{
  "name": "@danmasta/walk",
  "private": true,
  "version": "4.2.2",
  "author": "Daniel Smith <dannmasta@gmail.com>",
  "description": "Directory and file walking utility for node apps",
  "license": "MIT",
  "keywords": [
    "walk",
    "directory",
    "file",
    "util"
  ],
  "main": "index.js",
  "type": "module",
